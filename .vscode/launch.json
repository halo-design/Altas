{
  "version": "0.0.1",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Altas",
      "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron",
      "windows": {
        "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd"
      },
      "program": "${workspaceRoot}/renderer/main.js",
      "protocol": "inspector",
      "args": ["."],
      "env": {
        "NODE_ENV": "development"
      },
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Lanch Build Main",
      "args": ["${workspaceRoot}/bin/bundle.ts", "--mode", "main"],
			"runtimeArgs": ["-r", "ts-node/register"],
			"cwd": "${workspaceRoot}",
			"protocol": "inspector"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Lanch Pack Win",
      "args": ["${workspaceRoot}/node_modules/.bin/electron-builder", "--win"],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Lanch Pack Mac",
      "args": ["${workspaceRoot}/node_modules/.bin/electron-builder", "--mac"],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
      "outputCapture": "std"
    }
  ]
}
