{
  "name": "electron-altas",
  "appName": "Altas",
  "version": "0.0.4",
  "description": "An Electron Application",
  "main": "main.js",
  "scripts": {
    "update": "yarn upgrade-interactive --latest",
    "start": "electron .",
    "start:dev": "NODE_ENV=development electron .",
    "dev": "rm -rf app/static && parcel src/core/index.tsx --out-dir app/static --target electron",
    "build": "rm -rf app/static && parcel build src/core/index.tsx --out-dir app/static --no-source-maps --target electron",
    "pack": "npm run build && build --mac --win",
    "pack:mac": "npm run build && build --mac",
    "pack:win": "npm run build && build --win",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint -c tslint.json 'src/**/*.ts' --fix"
  },
  "keywords": [
    "Electron",
    "React",
    "TypeScript",
    "Parcel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/halo-design/Altas.git"
  },
  "author": "OwlAford",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "electron": "^4.0.1",
    "electron-builder": "^20.38.4",
    "node-sass": "^4.11.0",
    "parcel-bundler": "^1.11.0",
    "sass": "^1.15.3",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "electron-better-dialog": "^1.0.6",
    "electron-dl": "^1.12.0",
    "electron-json-storage": "^4.1.5",
    "electron-window-state": "^5.0.3",
    "fs-extra": "^7.0.1",
    "mobx": "^5.8.0",
    "mobx-react": "^5.4.3",
    "mobx-react-devtools": "^6.0.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "tslib": "^1.9.3"
  }
}
