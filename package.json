{
  "name": "electron-altas",
  "version": "0.0.2",
  "description": "A Electron application",
  "main": "main.js",
  "scripts": {
    "update": "yarn upgrade-interactive --latest",
    "start": "electron .",
    "dev": "rm -rf app/static && parcel src/core/index.tsx --out-dir app/static --target electron",
    "build": "rm -rf app/static && parcel build src/core/index.tsx --out-dir app/static --no-source-maps --target electron",
    "pack": "build --mac --win",
    "pack:mac": "build --mac",
    "pack:win": "build --win"
  },
  "keywords": [
    "Electron"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/halo-design/Altas.git"
  },
  "author": "OwlAford",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "electron": "^4.0.0",
    "electron-builder": "^20.38.4",
    "node-sass": "^4.11.0",
    "parcel-bundler": "^1.11.0",
    "sass": "^1.15.2",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "electron-window-state": "^5.0.3",
    "fs-extra": "^7.0.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "request": "^2.88.0",
    "request-progress": "^3.0.0"
  }
}
