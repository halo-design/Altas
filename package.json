{
  "name": "electron-altas",
  "version": "0.0.1",
  "description": "A Electron application",
  "main": "main.js",
  "scripts": {
    "dev": "rm -rf app/web/static && parcel src/core/index.tsx --out-dir app/web/static --target electron",
    "build": "rm -rf app/web/static && parcel build src/core/index.tsx --out-dir app/web/static --no-source-maps --target electron",
    "zip": "node ./config/zip",
    "pack": "rm -rf bin && electron-packager ./app Altas --platform=darwin --arch=x64 --out ./bin --app-version 0.0.1 --overwrite --icon=./app/favicon.icns",
    "dmg": "electron-installer-dmg ./bin/Altas-darwin-x64/Altas.app Altas --out=installer --icon=./app/favicon.icns --debug --overwrite"
  },
  "keywords": [
    "Electron"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/owlaford/Altas.git"
  },
  "author": "OwlAford",
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "archiver": "^3.0.0",
    "electron": "^4.0.0",
    "electron-installer-dmg": "^2.0.0",
    "electron-packager": "^13.0.1",
    "node-sass": "^4.11.0",
    "parcel-bundler": "^1.11.0",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  }
}
