    "electron-better-dialog": "^1.0.6",
    "electron-dl": "^1.14.0",
    "electron-json-storage": "^4.1.6",
    "electron-log": "^3.0.6",
    "electron-window-state": "^5.0.3",
    "face-api.js": "^0.20.1",
    "fs-extra": "^8.1.0",
    "github-markdown-css": "^3.0.1",
    "highlight.js": "^9.15.8",
    "ip": "^1.1.5",
    "lodash": "^4.17.11",
    "markdown-it": "^8.4.2",
    "markdown-it-attrs": "^2.4.1",
    "mobx": "^5.10.1",
    "mobx-react": "^6.1.1",
    "mobx-react-devtools": "6.1.1",
